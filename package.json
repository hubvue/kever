{
  "name": "kever",
  "version": "0.4.9",
  "description": "A lightweight inversion of control container for Node.js apps powered by TypeScript and Koa runtime",
  "scripts": {
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add ./CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubvue/kever.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "ioc",
    "http server",
    "koa"
  ],
  "author": "kim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hubvue/kever/issues"
  },
  "homepage": "https://github.com/hubvue/kever#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0"
  },
  "dependencies": {
    "@kever/core": "^0.1.7",
    "@kever/ioc": "^0.1.2",
    "@kever/logger": "^0.1.2",
    "@kever/router": "^0.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
